		<if test="token != null and token != '' ">
			and token = #{token}
		</if>
	</where>
</sql>
<select id="get" resultType="com.xyz.demo.pojo.User" >
	select		<include refid="Base_Column_List" />
	from t_user
	where id = #{id}
</select>

